<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	  package="edu.stanford.mobisocial.dungbeetle"
	  android:versionCode="12"
	  android:versionName="0.0.12">

  <uses-permission android:name="android.permission.BLUETOOTH" />
  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
  <uses-permission android:name="android.permission.READ_CONTACTS" />
  <uses-permission android:name="android.permission.GET_ACCOUNTS" />
  <uses-permission android:name="android.permission.INTERNET" />
  <uses-permission android:name="android.permission.NFC" />
  <uses-permission android:name="android.permission.VIBRATE"/>
  <uses-permission android:name="android.permission.CAMERA" />
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
  <uses-permission android:name="android.permission.RECORD_AUDIO" />

  <application 
      android:name="edu.stanford.mobisocial.dungbeetle.App"
      android:label="@string/app_name" android:icon="@drawable/icon">

    <activity android:name="DungBeetleActivity"
	      android:label="@string/app_name"
	      android:screenOrientation="portrait"
	      android:windowSoftInputMode="stateHidden">
      <intent-filter>
	<action android:name="android.intent.action.MAIN" />
	<category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
      
      <intent-filter>
    <action android:name="android.nfc.action.NDEF_DISCOVERED" />
	<action android:name="android.intent.action.VIEW" />
	<category android:name="android.intent.category.DEFAULT" />
	<data android:scheme="http" android:host="mobisocial.stanford.edu" android:pathPrefix="/dungbeetle" />
      </intent-filter>

 
    </activity>

	<activity android:name="FeedListActivity"
	      android:label="Dung Feeds"
	      android:screenOrientation="portrait">
    </activity>

	<receiver android:name="Publisher">
		<intent-filter>
			<action android:name="mobisocial.db.action.PUBLISH" />
		</intent-filter>
	</receiver>


    <activity android:name="edu.stanford.mobisocial.dungbeetle.StatusUpdateActivity"
	      android:label="Dungbeetle Status"
	      android:screenOrientation="portrait">
    </activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.ContactsActivity"
	      android:label="@string/app_name"
	      android:screenOrientation="portrait">
    </activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.FeedActivity"
	      android:label="@string/app_name"
	      android:screenOrientation="portrait">
    </activity>

    <provider android:name="edu.stanford.mobisocial.dungbeetle.DungBeetleContentProvider" 
	      android:authorities="edu.stanford.mobisocial.dungbeetle.DungBeetleContentProvider" />

    <service android:name="edu.stanford.mobisocial.dungbeetle.DungBeetleService"/>

    <receiver android:name="StartupIntentReceiver">
      <intent-filter>
	<action
	    android:name="android.intent.action.BOOT_COMPLETED" />
	<category android:name="android.intent.category.HOME" />
      </intent-filter>
    </receiver>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.HandleNfcContact"
	      android:label="@string/new_shared_contact"
	      android:screenOrientation="portrait">
      <intent-filter>
	<action android:name="edu.stanford.mobisocial.dungbeetle.HANDLE_NFC_CONTACT" />
	<category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
      <intent-filter>
	<action android:name="android.intent.action.VIEW" />
	<category android:name="android.intent.category.DEFAULT" />
	<category android:name="android.intent.category.BROWSABLE" />
	<data android:scheme="db-share-contact" android:host="dungbeetle" />
      </intent-filter>
    </activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.SendFileActivity"
	      android:label="@string/pick_contacts"
	      android:screenOrientation="portrait">

      <intent-filter>
	<action android:name="android.intent.action.SEND" />
	<category android:name="android.intent.category.DEFAULT" />
	<data android:mimeType="audio/x-mpegurl"/>
      </intent-filter>

      <intent-filter>
	<action android:name="android.intent.action.SEND" />
	<category android:name="android.intent.category.DEFAULT" />
	<data android:mimeType="audio/mpegurl"/>
      </intent-filter>

      <intent-filter>
	<action android:name="android.intent.action.SEND" />
	<category android:name="android.intent.category.DEFAULT" />
	<data android:mimeType="text/plain"/>
      </intent-filter>

      <intent-filter>
	<action android:name="edu.stanford.mobisocial.dungbeetle.INVITE" />
	<category android:name="android.intent.category.DEFAULT" />
      </intent-filter>

      <intent-filter>
	<action android:name="edu.stanford.mobisocial.dungbeetle.PICK_CONTACTS" />
	<category android:name="android.intent.category.DEFAULT" />
      </intent-filter>

    </activity>

    <activity android:name="WebAppActivity" android:label="@string/app_name"
	      android:theme="@android:style/Theme.NoTitleBar"
	      android:configChanges="orientation"
	      ></activity>

    <activity android:name="VoiceRecorderActivity" android:theme="@android:style/Theme.Dialog" android:screenOrientation="portrait"></activity> 
    <activity android:name="FeedActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity> 
    <activity android:name="ProfileActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity> 
    <activity android:name="AddGroupActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity> 
    <activity android:name="FriendsActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity>
    <activity android:name="GroupsActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity> 
    <activity 
    	android:name="GroupsTabActivity"
    	android:label="@string/app_name"
    	android:screenOrientation="portrait"
	    android:windowSoftInputMode="stateHidden">
	      
	      <intent-filter>
		      <action android:name="android.intent.action.VIEW"/>
		      <category android:name="android.intent.category.DEFAULT" />
		      <data android:mimeType="vnd.mobisocial.db/group" />
	      </intent-filter>
	      </activity> 
    <activity android:name="ViewContactActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity>
    <activity android:name="ViewContactTabActivity"  android:label="@string/app_name" android:screenOrientation="portrait"></activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.facebook.FacebookInterfaceActivity" 
	      android:label="@string/app_name"
	      android:screenOrientation="portrait"
	      android:configChanges="keyboardHidden">
    </activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.google.OAuthFlowApp" 
	      android:label="@string/app_name"
	      android:screenOrientation="portrait"
	      android:configChanges="keyboardHidden">
    </activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.ImageViewerActivity"
	      android:label="@string/app_name">
    </activity>

    <activity android:name="edu.stanford.mobisocial.dungbeetle.google.PrepareRequestTokenActivity" android:launchMode="singleTask">
      <intent-filter>
	<action android:name="android.intent.action.VIEW" />
	<category android:name="android.intent.category.DEFAULT" />
	<category android:name="android.intent.category.BROWSABLE" />
	<data android:scheme="x-oauthflow" android:host="callback" />
      </intent-filter>
    </activity>

    <activity
	android:label="DungBeetle"
	android:name="edu.stanford.mobisocial.dungbeetle.WebContentHandler">
      
      <!-- See DungBeetleActivity. Sorry!
      <intent-filter>
	<action android:name="android.intent.action.VIEW" />
	<category android:name="android.intent.category.DEFAULT" />
	<data android:scheme="http" android:host="mobisocial.stanford.edu" android:pathPrefix="/dungbeetle" />
      </intent-filter>
      -->
    </activity>
    
    
    
    <activity android:name="edu.stanford.mobisocial.dungbeetle.HandleGroupSessionActivity"
	      android:label="Group Session Invite"
	      android:screenOrientation="portrait">

      <intent-filter>
	<action android:name="edu.stanford.mobisocial.dungbeetle.HANDLE_GROUP_INVITE" />
	<category android:name="android.intent.category.DEFAULT" />
	<category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>

      <intent-filter>
	<action android:name="android.intent.action.VIEW" />
	<category android:name="android.intent.category.DEFAULT" />
	<category android:name="android.intent.category.BROWSABLE" />
	<data android:scheme="dungbeetle-group-session"/>
      </intent-filter>

      <intent-filter>
	<action android:name="android.nfc.action.NDEF_DISCOVERED" />
	<category android:name="android.intent.category.DEFAULT" />
	<data android:scheme="dungbeetle-group-session"/>
      </intent-filter>

    </activity>

   <receiver android:name="edu.stanford.mobisocial.dungbeetle.objects.ActivityPullObj">
     <intent-filter>
   	   <action android:name="mobisocial.intent.action.SET_NDEF" />
   	 </intent-filter>
   </receiver>

  </application>
</manifest> 
